{
  "ConnectionStrings": {
    "Postgres": "server=postgres;Database=employees;Uid=user;Pwd=simplepwd;"
  },

  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            },
            {
              "Name": "File",
              "Args": {
                "path": "%TEMP%\\Logs\\Findest.Api.log"
              }
            }
          ]
        }
      }
    ],
    "Enrich": ["FromLogContext", "WithMachineName"],
    "Properties": {
      "Application": "Findest.Api"
    }
  }
}
